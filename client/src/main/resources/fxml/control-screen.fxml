<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<Pane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.cuukenn.client.ui.controller.ControlScreen">
    <children>
        <MenuBar maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
            <menus>
                <Menu mnemonicParsing="false" text="操作">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#connect" text="connect server"/>
                        <MenuItem mnemonicParsing="false" onAction="#disconnect" text="disconnect server"/>
                        <MenuItem mnemonicParsing="false" onAction="#stopControl" text="stop control"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="关于">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Action 1"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <ImageView fx:id="image"/>
    </children>
</Pane>